{
    "name": "imbo/imbo-amqp-publisher",
    "description": "AMQP event publisher plugin for Imbo",
    "keywords": [ "metadata", "search", "imbo" ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Espen Hovlandsdal",
            "email": "espen@hovlandsdal.com"
        }
    ],
    "support": {
        "issues": "https://github.com/imbo/imbo-amqp-publisher",
        "irc": "irc://irc.freenode.net/imbo"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:imbo/imbo.git"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "php-amqplib/php-amqplib": "^2.6"
    },
    "require-dev": {
        "imbo/imbo": "dev-imbo-3.0",
        "ext-imagick": ">=3.0.1",
        "phpunit/phpunit": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Imbo\\Plugin\\AmqpPublisher\\": "src/",
            "ImboUnitTest\\Plugin\\AmqpPublisher\\": "tests/unit",
            "ImboIntegrationTest\\Plugin\\AmqpPublisher\\": "tests/integration"
        }
    }
}
